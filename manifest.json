{
    "manifest_version": 2,
    "name": "english-spanish-cambridge",
    "description": "When select a word, open a pop-up panel with de 'Cambridge English-Spanish dictionary'",
    "version": "1.0",
    "homepage_url": "https://github.com/smuxdev",

    "icons": {
        "32": "icons/dictionary-32.png",
        "48": "icons/dictionary-48.png"
    },

    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["content-script.js"]
    }],

    "background": {
        "scripts": ["background.js"]
    }
}
